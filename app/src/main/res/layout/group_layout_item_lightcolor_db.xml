<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="50dp">

    <!--<android.support.v7.widget.CardView-->
    <!--android:id="@+id/cv_group_lc_db"-->
    <!--style="@style/SameCornerRadiusCardview"-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="match_parent">-->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        >

        <CheckBox
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:clickable="false"
            android:id="@+id/db_light_color_stand"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <TextView
            android:id="@+id/tv_name_group_lc_db"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:textSize="@dimen/eplv_group_textsize"/>

        <!--<TextView-->
        <!--android:id="@+id/tv_tips_group_lc_db"-->
        <!--android:layout_width="0dp"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:layout_weight="1"/>-->

        <TextView
            android:id="@+id/tv_date_group_lc_db"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:textSize="@dimen/eplv_group_textsize"/>

        <TextView
            android:id="@+id/tv_time_group_lc_db"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:textSize="@dimen/eplv_group_textsize"/>

        <TextView
            android:id="@+id/tv_result_group_lc_db"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:textSize="@dimen/eplv_group_textsize"
            android:visibility="gone"/>
    </LinearLayout>

    <!--</android.support.v7.widget.CardView>-->

</FrameLayout>